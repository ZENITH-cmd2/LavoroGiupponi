{
    "version": 2,
    "builds": [
        {
            "src": "backend/server.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/frontend/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/backend/server.py"
        }
    ]
}